DIR='/scratch/scheucher/workspace/baci_springfitting/transfer/aero_damping'
PRE='/scratch/scheucher/workspace/baci_springfitting_release/pre_exodus'
MAIN='/scratch/scheucher/workspace/baci_springfitting_release/baci-release'
POST='/scratch/scheucher/workspace/baci_springfitting_release/post_processor'

rm $DIR/out/a*
rm $DIR/ens/a*
rm $DIR/dat/a*



$PRE --exo=$DIR/plate.exo --bc=$DIR/aerospring.bc --head=$DIR/aerospring.head --dat=$DIR/dat/aerospring.dat

mpirun -np 3 $MAIN $DIR/dat/aerospring.dat $DIR/out/aerospring

$POST --filter=ensight --file=$DIR/out/aerospring --output=$DIR/ens/aerospring --stress=cxyz_ndxyz

